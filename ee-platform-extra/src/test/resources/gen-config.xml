<?xml version="1.0" encoding="UTF-8" ?>
<config>

    <genProperty>
        <packageName>com.kyyblabla</packageName>
        <!--表名前缀匹配模式如：表"sys_user" 的前缀为"sys_" -->
    </genProperty>

    <!--jdbc配置，需要客户端配置-->
    <jdbcConnection>
        <driverClassName>com.mysql.jdbc.Driver</driverClassName>
        <url>jdbc:mysql://localhost:3306/ee-platform</url>
        <username>root</username>
        <password>123</password>
    </jdbcConnection>

    <!--需要生成的数据库表定义 begin-->
    <schemes>
        <scheme name="user" moduleName="sys" subMoudle="user">
            <table tableName="user" className="User"></table>
        </scheme>
    </schemes>
    <!--需要生成的数据库表定义 end-->


</config>