<?xml version="1.0" encoding="UTF-8" ?>
<genCofig>

    <!--生成计划 begin-->
    <plans>
        <plan name="jpa-dao" comment="jpa类型的dao">
            <template>jpa-dao/Entity.java</template>
            <template>jpa-dao/EntityDao.java</template>
        </plan>
        <plan name="jpa-dao-test" comment="jpa类型的dao测试">
            <template>jpa-dao-test/EntityDaoTest.java</template>
        </plan>
        <plan name="crud" comment="crud使用jpa">
            <template>crud/EntityService.java</template>
            <template>crud/EntityController.java</template>
        </plan>
        <plan name="crud-test" comment="crud使用jpa">
            <template>crud-test/EntityServiceTest.java</template>
            <template>crud-test/EntityControllerTest.java</template>
        </plan>
        <plan name="crud-jpa" comment="crud使用jpa">
            <template>REF:crud</template>
            <template>REF:jpa-dao</template>
        </plan>
        <plan name="crud-jpa-with-test" comment="crud使用jpa+测试">
            <template>REF:crud-jpa</template>
            <template>REF:jpa-dao-test</template>
            <template>REF:crud-test</template>
        </plan>
    </plans>
    <!--生成计划 end-->


    <!--类型处理器 begin-->
    <typeHandles>
        <handle name="genderEnumHandler">genderEnumHandler</handle>
    </typeHandles>
    <!--类型处理器 end-->

    <!--需要生成的数据库表定义 begin-->
    <tables>
        <table name="user" moduleName="system" subMoudle="user" className="User" classNameFll="user">
            <columnOverride columnName="gender" columnType="" attributeName="gender" attributeType="Enum"
                            typeHandle="genderEnumHandler"/>
        </table>
    </tables>
    <!--需要生成的数据库表定义 end-->


</genCofig>